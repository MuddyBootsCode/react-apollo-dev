{
  "name": "apollo-template",
  "version": "1.0.0",
  "description": "Apollo Server Template",
  "main": "index.js",
  "repository": "https://github.com/MuddyBootsCode/apollo-server-template.git",
  "author": "Muddy Boots Code",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "DEBUG=neo4j-graphql-js ./node_modules/.bin/nodemon --watch src --ext js,graphql --exec babel-node  index.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build index.js graphql-schema.js",
    "build": "npm run clean && npm run build-babel && cp schema.graphql ./build",
    "start": "npm run build && node ./build/index.js"
  },
  "dependencies": {
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-server-express": "^2.18.1",
    "apollo-server-lambda": "^2.13.1",
    "babel-plugin-import-graphql": "^2.7.0",
    "csv-parse": "^4.10.1",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "graphql-tag": "^2.10.1",
    "neo4j-driver": "^4.0.2",
    "neo4j-graphql-js": "^2.14.2",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime-corejs3": "^7.9.2",
    "babel-plugin-auto-import": "^1.0.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "cross-env": "^7.0.2",
    "nodemon": "^1.19.1",
    "shx": "^0.3.2"
  }
}
